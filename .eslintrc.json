{
  "extends": "airbnb",
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "linebreak-style": 0,
    "no-debugger": 0,
    "react/jsx-boolean-value": 0,
    "react/forbid-prop-types": 0
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"]
      }
    }
  }
}